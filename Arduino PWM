/*PWM-esimerkki 1
 * Date: 20.6.2022
 * Author: Niina Paukkonen
 * Comments: kokeillaan PWM lahtoa
 */


byte ledit1 = 6; //pwm pinnit 3,5,6,9,10,11


void setup() {


}

void loop() {

analogWrite(ledit1, 255); //pulssisuhde 100%
delay(100);
analogWrite(ledit1, 191); //pulssisuhde 75%
delay(100);
analogWrite(ledit1, 127); //pulssisuhde 50%
delay(100);
analogWrite(ledit1, 63); //pulssisuhde 25%
delay(100);
analogWrite(ledit1, 0);
delay(100);

}
