#include <iostream>
#include <string>
using namespace std;

int main()
{
	//kokonaislukutaulukon määrittely, viisi alkiota
	const int t_koko = 5;
	int ekaTaulukko[t_koko];
	string autot[4] = { "Ford", "Audi", "Lada", "Tesla" };

	ekaTaulukko[0] = 12;
	ekaTaulukko[1] = 22;
	ekaTaulukko[2] = 2;
	ekaTaulukko[3] = 45;
	ekaTaulukko[4] = 34;

	for(int i = 0; i < t_koko; i++)
	{
		cout << ekaTaulukko[i] << endl;
	}

	for (int i = 0; i < 5; i++)
	{

		cout << autot[i] << endl;
	}

	return 0;
}
